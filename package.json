{
  "name": "amneziawg-documentation-portal",
  "version": "1.1.0",
  "description": "Documentation portal for AmneziaWG Docker VPN Server",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "npm run dev:full",
    "dev": "npm run dev:full",
    "dev:api": "tsx server/main.ts",
    "dev:vite": "vite",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev:vite\"",
    "start:prod": "NODE_ENV=production tsx server/main.ts",
    "build": "npm run build:vite",
    "build:vite": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "preview": "vite preview"
  },
  "keywords": [
    "vpn",
    "amneziawg",
    "docker",
    "documentation"
  ],
  "author": "asychin",
  "license": "MIT",
  "dependencies": {
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "lucide-react": "^0.554.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
